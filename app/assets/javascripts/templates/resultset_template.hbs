<table class="table table-bordered table-striped" id="resultset">
  {{#each IWitness.ResultsetController.content}}
    <tr>
      <td>
        <strong>@{{this.from_user}}</strong>
        <em>{{this.timestamp}}</em>
        <p>{{this.text}}</p>
      </td>
    </tr>
  {{/each}}

  {{#unless IWitness.ResultsetController.content}}
    <tr>
      {{#if IWitness.ResultsetController.searching}}
        <td class="message">Searching...</td>
      {{else}}
        <td class="message">No Results</td>
      {{/if}}
    </tr>
  {{/unless}}
</table>
