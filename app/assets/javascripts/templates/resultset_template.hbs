<table class="table table-bordered table-striped" id="resultset">
  {{#each IWitness.ResultsetController.content}}
    <tr>
      <td class="pic">
        <img {{bindAttr src="profile_image_url"}}>
      </td>
      <td>
        <strong><a target="_blank" {{bindAttr href="from_user_url"}}>@{{from_user}}</a></strong>
        <em>{{timestamp}}</em>
        <p>{{linkify text}}</p>
      </td>
    </tr>
  {{/each}}

  {{#unless IWitness.ResultsetController.content.length}}
    <tr>
      {{#if IWitness.ResultsetController.searching}}
        <td class="message">Searching...</td>
      {{else}}
        <td class="message">No Results</td>
      {{/if}}
    </tr>
  {{/unless}}
</table>
